{
  "name": "@thesara/api",
  "private": true,
  "version": "0.0.3",
  "type": "commonjs",
  "main": "dist/server.cjs",
  "scripts": {
    "dev": "cross-env CREATEX_WORKER_ENABLED=true NODE_OPTIONS=\"--openssl-legacy-provider --require ts-node/register/transpile-only --require ./src/preload.ts\" tsx watch src/index.ts",
    "dev:worker": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider --require ts-node/register/transpile-only --require ./src/preload.ts\" tsx watch src/localdev/worker.run.ts",
  "build": "tsup && cpx \"src/**/!(*.ts)\" dist && node -e \"const fs=require('fs'); fs.copyFileSync('dist/index.js','dist/server.cjs')\"",
  "verify:dist": "node ./scripts/verify-dist-no-update.js",
  "postbuild": "pnpm run verify:dist",
    "build:tsc": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.f2.json",
    "start": "node --openssl-legacy-provider dist/index.js",
    "migrate": "cross-env NODE_OPTIONS=--openssl-legacy-provider tsx src/migrate.ts",
    "check:bundle": "node -e \"import('file://'+process.cwd()+'/dist/server.mjs').then(()=>console.log('OK')).catch(e=>{console.error(e);process.exit(1)})\"",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:dev:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed",
    "postinstall": "prisma generate",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "prestart": "pnpm run build",
    "createx:build": "node -e \"console.log('createx:build placeholder')\""
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.585.0",
    "@aws-sdk/lib-storage": "^3.585.0",
    "@babel/parser": "7.28.4",
    "@babel/traverse": "7.28.4",
    "@babel/types": "7.28.4",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "10.0.0",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "9.5.2",
    "@fastify/swagger-ui": "5.2.3",
    "@google-cloud/storage": "^7.7.0",
    "@loopyway/entitlements": "workspace:*",
    "@prisma/client": "6.18.0",
    "@swc/helpers": "0.5.17",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "archiver": "^7.0.1",
    "argon2": "0.44.0",
    "autoprefixer": "^10.4.19",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.8.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.6.1",
    "es-module-lexer": "1.7.0",
    "esbuild": "^0.21.5",
    "fastify": "^5.5.0",
    "fastify-plugin": "^5.0.1",
    "fastify-raw-body": "^5.0.0",
    "firebase-admin": "13.5.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "pdfkit": "0.17.2",
    "postcss": "^8.4.33",
    "prom-client": "15.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^14.25.0",
    "tailwindcss": "^3.4.0",
    "tar": "^7.4.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
  "@types/react": "19.1.9",
  "@types/react-dom": "19.1.7",
    "@fastify/multipart": "^9.0.3",
    "@types/archiver": "^5.3.4",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "24.2.1",
    "@types/nodemailer": "^7.0.0",
    
    "cross-env": "^7.0.3",
    "dotenv-cli": "^10.0.0",
    "extract-zip": "^2.0.1",
    "framer-motion": "^11.0.0",
    "globby": "^14.1.0",
    "prisma": "6.18.0",
    "recharts": "^2.12.7",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsup": "^8.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.4.0",
    "puppeteer": "^24.16.0"
  }
}
