# Example production env for Web (Next.js)
NODE_ENV=production
PORT=3000
INTERNAL_API_URL=http://127.0.0.1:8788/api
NEXT_PUBLIC_API_BASE_URL=https://thesara.space/api
NEXT_PUBLIC_API_URL=https://thesara.space/api
NEXT_PUBLIC_APPS_HOST=https://thesara.space

# If you need additional public keys/placeholders, add them here
# e.g. NEXT_PUBLIC_SENTRY_DSN=\n# Used for signing JWTs for client-side rooms access if ROOMS_V1__JWT_SECRET is not set.
JWT_SECRET="your_jwt_secret_here"

# Used for signing JWTs for client-side rooms access. Overrides JWT_SECRET if set.
ROOMS_V1__JWT_SECRET="your_rooms_v1_jwt_secret_here"

# The issuer for the JWTs.
JWT_ISSUER="your_jwt_issuer_here"

# The audience for the JWTs.
JWT_AUDIENCE="your_jwt_audience_here"

# API base URL for production
NEXT_PUBLIC_API_BASE_URL="https://api.thesara.space/api"
NEXT_PUBLIC_APPS_HOST="https://apps.thesara.space"
