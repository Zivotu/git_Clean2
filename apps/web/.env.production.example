# Example production env for Web (Next.js)
NODE_ENV=production
PORT=3000
INTERNAL_API_URL=http://127.0.0.1:8788/api
NEXT_PUBLIC_API_BASE_URL=https://thesara.space/api
NEXT_PUBLIC_API_URL=https://thesara.space/api
NEXT_PUBLIC_APPS_HOST=https://thesara.space

# If you need additional public keys/placeholders, add them here
# e.g. NEXT_PUBLIC_SENTRY_DSN=\n# Used for signing JWTs for client-side rooms access if ROOMS_V1__JWT_SECRET is not set.
JWT_SECRET="your_jwt_secret_here"

# Used for signing JWTs for client-side rooms access. Overrides JWT_SECRET if set.
ROOMS_V1__JWT_SECRET="your_rooms_v1_jwt_secret_here"

# The issuer for the JWTs.
JWT_ISSUER="your_jwt_issuer_here"

# The audience for the JWTs.
JWT_AUDIENCE="your_jwt_audience_here"

# API base URL for production
NEXT_PUBLIC_API_BASE_URL="https://api.thesara.space/api"
NEXT_PUBLIC_APPS_HOST="https://apps.thesara.space"

# Golden Book donations (optional)
NEXT_PUBLIC_GOLDEN_BOOK_ENABLED=true
NEXT_PUBLIC_GOLDEN_BOOK_PAYMENT_LINK=https://buy.stripe.com/prod
NEXT_PUBLIC_GOLDEN_BOOK_PRICE_ID=price_xxx
NEXT_PUBLIC_GOLDEN_BOOK_PRODUCT_ID=prod_xxx
NEXT_PUBLIC_GOLDEN_BOOK_CAMPAIGN_ID=goldenbook-2025-q1
NEXT_PUBLIC_GOLDEN_BOOK_CAMPAIGN_START_MS=
NEXT_PUBLIC_GOLDEN_BOOK_CAMPAIGN_END_MS=
NEXT_PUBLIC_GOLDEN_BOOK_ALIAS_GRACE_MS=86400000

# AdSense slot identifiers (required for production rollout)
NEXT_PUBLIC_ADS_SLOT_PLAY_TOP=
NEXT_PUBLIC_ADS_SLOT_PLAY_BOTTOM=
NEXT_PUBLIC_ADS_SLOT_APP_HEADER=
NEXT_PUBLIC_ADS_SLOT_APP_INLINE=
NEXT_PUBLIC_ADS_SLOT_HOME_RAIL_LEFT=
NEXT_PUBLIC_ADS_SLOT_HOME_RAIL_RIGHT=
NEXT_PUBLIC_ADS_SLOT_HOME_GRID_INLINE=
NEXT_PUBLIC_ADS_SLOT_HOME_FEED_FOOTER=
NEXT_PUBLIC_ADS_SLOT_MARKETPLACE_GRID_INLINE=
